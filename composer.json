{
  "name": "nettrine/extensions-oroinc",
  "description": "Doctrine2 DQL functions and field types for Nette Framework",
  "keywords": ["nette", "doctrine", "orm", "dbal", "database"],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/nettrine/extensions-oroinc",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "doctrine/dbal": "^2.10.0",
    "nette/di": "~3.0.0",
    "oro/doctrine-extensions": "^1.3.0"
  },
  "require-dev": {
    "ninjify/nunjuck": "^0.3.0",
    "doctrine/orm": "^2.5.0",
    "ninjify/qa": "^0.12.0",
    "phpstan/extension-installer": "^1.0",
	"phpstan/phpstan": "^0.12",
    "phpstan/phpstan-deprecation-rules": "^0.12",
    "phpstan/phpstan-nette": "^0.12",
    "phpstan/phpstan-strict-rules": "^0.12"
  },
  "autoload": {
    "psr-4": {
      "Nettrine\\Extensions\\Oroinc\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Cases\\": "tests/cases",
      "Tests\\Toolkit\\": "tests/toolkit"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.1.x-dev"
    }
  }
}
